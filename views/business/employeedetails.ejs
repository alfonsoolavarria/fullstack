<body class="animsition page-user">
  <!-- Headear barra -->
  <% include ../templates/header.ejs %>
  <!-- Menu-->
  <% include ../templates/menuvertical.ejs %>
  <div class="page">
    <div class="page-content">
      <div class="col-xl-12">
        <div class="panel">
          <div class="panel-body">
            <form class="page-search-form" role="search"></form>
              <div class="nav-tabs-horizontal nav-tabs-animate" data-plugin="tabs">
                <div class="tab-content">
                  <div class="tab-pane animation-fade active" id="all_contacts" role="tabpanel">
                    <div class="media">
                      <div class="media-body">
                        <form class="form-horizontal">
                          <div class="row" style="margin-left:10px;">
                            <div class="col-md-3"></div>
                            <div class="col-md-3"><label><b>Nombre</b></label></div>
                            <div class="col-md-3"><label><b>Email</b></label></div>
                            <div class="col-md-3"><label><b>Tel&eacute;fono</b></label></div>
                          </div>
                          <div class="row">
                            <div class="col-md-3 iconstyle2 icon-empl-<%=employeeB.objectId%>">
                              <%if (employeeB.image){%>
                                <input type="file" id="input-file-now-icon" class="iconIcon2" data-plugin="dropify" data-default-file="<%=employeeB.image.url%>" disabled/>
                              <%}else{%>
                                <input type="file" id="input-file-now-icon" class="iconIcon2" data-plugin="dropify" data-default-file="/public/assets/images/avataremployee.png" disabled/>
                              <%}%>
                            <i class="avatar avatar-busy"></i>
                            </div>
                            <div class="col-md-3">
                              <input class="form-control" id="emplo-name-<%=employeeB.objectId%>" style="border:0px;background:none;" value="<%=employeeB.name%>" disabled></input>
                            </div>
                            <div class="col-md-3">
                              <input class="form-control" id="emplo-email-<%=employeeB.objectId%>" style="border:0px;background:none;width:140%;" value="<%=employeeB.username%>" disabled></input>
                            </div>
                            <div class="col-md-3">
                              <input class="form-control" id="emplo-phone-<%=employeeB.objectId%>" style="border:0px;background:none;" value="<%=employeeB.phone%>" disabled></input>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-md-5"></div>
                            <div class="col-md-6">
                              <%if (employeeB.isActive==false){%>
                                <button type="button" class="btn btn-primary btn-warning disabledEmpl waves-effect" data-activate-id="<%=employeeB.objectId%>" id="confirmA-<%=employeeB.objectId%>" style="height:32px;">Activar</button>
                              <%}else{%>
                                <button type="button" class="btn btn-primary btn-sm edit btn-<%=employeeB.objectId%>" data-edit-id="<%=employeeB.objectId%>" ><span id="nameE-<%=employeeB.objectId%>">Editar</span></button>
                                <button type="button" class="btn btn-primary btn-danger waves-effect" data-delete-id="<%=employeeB.objectId%>" id="confirm-<%=employeeB.objectId%>" style="height:32px;">Eliminar</button>
                              <%}%>
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
                    <button type="button" class="btn btn-sm btn-primary waves-effect" id="employeeDelay" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Datos Enviados" style="visibility:hidden;" >delay</button>
                    <button type="button" class="btn btn-sm btn-primary waves-effect" id="emplook" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Borrado" style="visibility:hidden;" >delay</button>
                    <button type="button" class="btn btn-sm btn-primary waves-effect" id="emploDesactivate" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Activado" style="visibility:hidden;" >delay</button>
                    <button type="button" class="btn btn-sm btn-primary waves-effect" id="emplocancel" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Cancelada" style="visibility:hidden;" >delay</button>
                    <button type="button" class="btn btn-primary waves-effect" id="employeeConflict" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Ya este Email Existe para otro Propietario " style="visibility:hidden;">delay</button>
                    <button type="button" class="btn btn-primary waves-effect" id="EditEmployeeSave" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Se guardo con Exito" style="visibility:hidden;" >delay</button>
                    <button type="button" class="btn btn-primary waves-effect" id="EditEmpleoyeeError" data-plugin="alertify"
                      data-type="log" data-delay="3000" data-log-message="Error al guardar" style="visibility:hidden;" >delay</button>
                    <input id="flagPaginator" value="2" style="visibility:hidden;">
                    <input id="Bid" value="<%if (session.owner || session.ownerAdmin){%><%=session.userId%><%}else{%><%=bussiId%><%}%>" style="visibility:hidden;">
                    <input id="BidFlag" value="<%if (session.owner || session.ownerAdmin){%>1<%}else{%>2<%}%>" style="visibility:hidden;">
                  </div>
                </div>
              </div>
            </div>
        </div>
      </div>
    </div>
  </div>
<!-- Site Action Nuevo Empleado-->
</div>


